{
  "name": "The Vault Extension",
  "version": "1.0",
  "description": "Passwords Keeper",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "tabs",
    "input",
    "management",
    "storage",
    "*://*/*", "http://*/*", "https://*/*"
  ],
  "background": {
    "scripts": [
      "background.js",
      "runtime.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "index.html",
        "default_icon": {
          "32": "assets/icons/vault.png"
        }
  }



}
