{
  "name": "The Vault Extension",
  "version": "1.0",
  "description": "Passwords keeper",
  "manifest_version": 2,
  "browser_action": {
    "default_icon":{
      "32": "assets/icons/vault.png"
    },
    "permissions": [
      "activeTab"
    ],
    "background": {
      "scripts": [
        "background.js",
        "runtime.js"
      ]
    },
    "default_popup": "index.html"
  }
}
